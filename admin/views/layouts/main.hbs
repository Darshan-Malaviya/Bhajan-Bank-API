    <script>
        $(".datetimeinput").attr("type", "datetime-local");

        $(function () {
            var path = location.pathname; // because the 'href' property of the DOM element is the absolute path
            $(".nav-item").removeClass("active");
            $(".nav-item a").each(function () {
                var $this = $(this);
                if ($this.attr("href") == path) {
                    $(this).parents(".nav-item").addClass("active");
                }
            });
        });

        var alerts = document.getElementsByClassName("alert");
        for (let i = 0; i < alerts.length; i++) {
            const element = alerts[i];
            setTimeout(function () {
                element.remove();
            }, 10000);
        }

        $(".alert").each(function () {
            var $this = $(this);
            $this.on("click", function () {
                $this.toggle();
            });
        });

        $('#zero_config').dataTable({
            "columnDefs": [
                { "sortable": false, "targets": [0, 3, 4] }
            ],
        });

        $(".download-btn").each(function () {
            var $this = $(this);
            $this.attr("href");
            if ($this.attr("href") == "/media/" || $this.attr("href") == "") {
                $this.remove();
            }
        });

        $('.deletebtn').on('click', function () {
            var $this = $(this);
            var hrefattr = $this.attr('data-href');
            swal({
                title: 'Are you sure?',
                text: 'Once deleted, you will not be able to redo this action',
                icon: 'warning',
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        location.replace(hrefattr)
                    } else {
                        swal('Your data is safe');
                    }
                });
        });


        $("#zero_config_length").css("margin-top", "10px");

        $("input").addClass("form-control");
        $('input[type="checkbox"]').removeClass("form-control");

        $("i.fas.fa-eye-slash").each(function () {
            let $this = $(this);
            let parentElement = $this.parent("*");
            parentElement.on("mouseover", function () {
                $this.toggleClass("fa-eye-slash");
                $this.toggleClass("fa-eye");
            });
            parentElement.on("mouseout", function () {
                $this.toggleClass("fa-eye-slash");
                $this.toggleClass("fa-eye");
            });
        });



        $('.modal-view-btn').on('click', function () {
            $('.modal').each(function () {
                var $element = $(this);
                console.log($element);
                $element.remove();
            });
            var $this = $(this);
            var book = $this.attr('data-book');
            var image = $this.attr('data-image');
            $this.fireModal({
                title: `<h5> Book : ${book} <br> Image : <a href="${image}" target="_blank">${image}</a></h5>`,
                body: `<img src="${image}" width="100% ">`
            });
        })
    </script>
